{
    "manifest_version": 2,
    "name": "Bookmark",
    "description": "This extension will analyze a page using222",
    "version": "1.0",
    "browser_action": {
        "default_icon": "avatar.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab", "bookmarks"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
